<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Sortir.com{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            <link rel="stylesheet" href="{{ asset('css/stylsheet.css') }}">
        {% endblock %}
        <link rel="icon" href="{{ asset('img/favicon.png') }}">
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>

    <body>
        <header>
            <div class="container">
                    <img class="header-logo" src="{{ asset('img/logo.png') }}" alt="logo">
                    {% include('inc/nav.html.twig') %}
            </div>
        </header>

        {% block body %}{% endblock %}
    </body>
</html>
